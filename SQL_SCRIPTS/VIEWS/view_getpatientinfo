DELIMITER $$
DROP VIEW IF EXISTS view_getpatientinfo;
CREATE VIEW view_getpatientinfo AS 
SELECT id AS "ID Number",
 LastName AS "Last Name",
 FirstName AS "First Name",
 MiddleName AS "Middle Name",
 Department_id AS "Department",
 Course_id AS "Course",
 Level AS "Level",
 Sex AS "SEx",
 Age AS "AGE"
 from patient;$$
 DELIMITER ;
